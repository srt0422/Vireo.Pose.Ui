{
  "name": "PoseUi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test-native": "jest",
    "test-web": "babel transformJsxFiles.babel.js -o transformJsxFiles.js & node transformJsxFiles __tests__ app/lib & mocha ./__tests__/web/**/*.js ./__tests__ --compilers js:babel-core/register",
    "test-web-dev": "babel transformJsxFiles.babel.js -o transformJsxFiles.js & node transformJsxFiles __tests__ app/lib & mocha ./__tests__/web/**/*.js ./__tests__ --compilers js:babel-core/register",
    "ios-bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "NODE_ENV=production tsc --jsx 'preserve'; babel transformJsxFiles.babel.js -o transformJsxFiles.js & node transformJsxFiles; react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "android-bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "start-ios": "NODE_ENV=production; react-native run-ios",
    "web-dev": "webpack-dev-server --config web/webpack/webpack.config.babel.js --progress --colors --open --inline --hot",
    "web-dev-bundle": "webpack --config web/webpack/webpack.config.babel.js --progress --colors",
    "web-bundle": "set NODE_ENV=production&& webpack -p --config web/webpack/webpack.production.config.babel.js --progress --colors",
    "clear-cache": "gulp clear-cache"
  },
